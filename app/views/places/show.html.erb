<!-- place detail page -->
<h1> <%= @place["name"]%></h1>

<ul>
  <% @entries.each do |entry| %> <!---added this --->
    <li>
    <h3> <%= entry["title"] %></h3>
    <p><%= entry["occurred_on"] %></p>
    <p><%= entry["description"] %></p>
    </li>
  <% end %>
</ul>

<p> <a href="/places/<%= @place.id %>/entries/new">New Post</a> </p>

<p> <a href="/places"">Back to All Places</a> </p>
